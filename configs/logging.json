{
  "version": 1,
  "formatters": {
    "default": {
      "format": "%(asctime)s [%(levelname)s] %(module)s: %(message)s"
    },
    "payload_format": {
      "format": "%(asctime)s:\n%(message)s"
    }
  },
  "handlers": {
    "file": {
      "class": "logging.FileHandler",
      "filename": "log/jenkins-trigger.log",
      "level": "INFO",
      "formatter": "default"
    },
    "payload_file": {
      "class": "logging.FileHandler",
      "filename": "log/payload.log",
      "level": "INFO",
      "formatter": "payload_format"
    }
  },
  "loggers": {
    "payload_logger": {
      "level": "INFO",
      "handlers": [
        "payload_file"
      ]
    },
    "logger": {
      "level": "INFO",
      "handlers": [
        "file"
      ]
    }
  }
}